# Database Configuration for DUI Views Creator
# Copy this file to .env and update with your actual database settings

# Database Server (default: localhost)
DB_SERVER=localhost

# Database Name (default: LEADRS_DUI_STAGE)
DB_DATABASE=LEADRS_DUI_STAGE

# Database Authentication
# Option 1: Windows Authentication (leave username/password empty)
DB_USERNAME=
DB_PASSWORD=

# Option 2: SQL Server Authentication (uncomment and fill in)
# DB_USERNAME=your_username
# DB_PASSWORD=your_password

# ODBC Driver (default: ODBC Driver 17 for SQL Server)
# If you have a different driver, update the connection string in create_dui_views.py 